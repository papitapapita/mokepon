{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "warn"
  },
  "plugins": ["jest"]
}
